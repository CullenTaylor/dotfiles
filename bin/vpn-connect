#!/usr/bin/env bash

CSD_WRAPPER="/usr/share/ca-certificates/ibm/ohsd.py"
CERT_PATH="/usr/share/ca-certificates/ibm/cert.pem"
KEY_PATH="/usr/share/ca-certificates/ibm/key.pem"

sudo openconnect -c $CERT_PATH              \
                 -k $KEY_PATH               \
                 --csd-wrapper=$CSD_WRAPPER \
                 --no-xmlpost -v sasvpn.pok.ibm.com
