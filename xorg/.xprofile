# set keyboar7d repeat rate
xset r rate 200 60

# make sure ~/.Xresources is loaded
xrdb -merge ~/.Xresources

# turn off Thinkpad touchpad
# synclient TouchpadOff=1
# synclient PalmDetect=1

# startup applications
compton --refresh-rate 60 &
#dockd --daemon &
nm-applet &
redshift-gtk &
xscreensaver &
